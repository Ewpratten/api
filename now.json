{
    "version": 2,
    "name": "api",
    "builds": [
        {
            "src": "**/*.py",
            "use": "@now/python@canary"
        },
        {
            "src": "apidocs/**",
            "use": "@now/static"
        },
        {
            "src": "apidocs/swagger.yaml",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/tvdsb/student/auth",
            "dest": "/router",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/tvdsb/student/attendance",
            "dest": "/router",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/tvdsb/student/marks",
            "dest": "/router",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/tvdsb/student/payment",
            "dest": "/router",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/tvdsb/student/timetable",
            "dest": "/router",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/frc/5024/lib5k/version",
            "dest": "/router",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}